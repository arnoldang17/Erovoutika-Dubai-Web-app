	>
	  Back
	</button>
  </div>
  
  <!-- Card Container -->
  <div id="cardsContainer" class="grid gap-4 mt-6 justify-center items-center"></div>
  
  <!-- Script -->
  <script>
	const addCardBtn = document.getElementById('addCardBtn')!;
	const cardForm = document.getElementById('cardForm')!;
	const cancelBtn = document.getElementById('cancelBtn')!;
	const submitCard = document.getElementById('submitCard')!;
	const cardsContainer = document.getElementById('cardsContainer')!;
	const backBtn = document.getElementById('backBtn')!;
	const backBtnContainer = document.getElementById('backBtnContainer')!;
  
	const inputTitle = document.getElementById('inputTitle') as HTMLInputElement;
	const inputDate = document.getElementById('inputDate') as HTMLInputElement;
	const inputMessage = document.getElementById('inputMessage') as HTMLTextAreaElement;
	const inputImage = document.getElementById('inputImage') as HTMLInputElement;
  
	addCardBtn.addEventListener('click', () => {
	  cardForm.classList.remove('hidden');
	});
  
	cancelBtn.addEventListener('click', () => {
	  cardForm.classList.add('hidden');
	  clearForm();
	});
  
	submitCard.addEventListener('click', () => {
	  const title = inputTitle.value.trim();
	  const date = inputDate.value;
	  const message = inputMessage.value.trim();
	  const imageFile = inputImage.files?.[0];
  
	  if (!title || !date || !message) {
		alert("All fields are required!");
		return;
	  }
  
	  const card = document.createElement('div');
	  card.className = "bg-white p-4 rounded shadow max-w-md";
  
	  if (imageFile) {
		const reader = new FileReader();
		reader.onload = function (e) {
		  const imageUrl = e.target?.result;
		  card.innerHTML = `
			<img src="${imageUrl}" alt="Uploaded Image" class="mb-2 w-full h-auto rounded" />
			<p class="text-sm text-gray-500">${date}</p>
			<h3 class="text-lg font-bold">${title}</h3>
			<p>${message}</p>
		  `;
		  cardsContainer.innerHTML = '';
		  cardsContainer.appendChild(card);
		  backBtnContainer.classList.remove('hidden');
		};
		reader.readAsDataURL(imageFile);
	  } else {
		card.innerHTML = `
		  <p class="text-sm text-gray-500">${date}</p>
		  <h3 class="text-lg font-bold">${title}</h3>
		  <p>${message}</p>
		`;
		cardsContainer.innerHTML = '';
		cardsContainer.appendChild(card);
		backBtnContainer.classList.remove('hidden');
	  }
  
	  cardForm.classList.add('hidden');
	  clearForm();
	});
  
	backBtn.addEventListener('click', () => {
	  location.reload();
	});
  
	function clearForm() {
	  inputTitle.value = "";
	  inputDate.value = "";
	  inputMessage.value = "";
	  inputImage.value = "";
	}
  </script>
  
  
  
  

      



